<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frame_10021"
    android:orientation="vertical"
    android:paddingStart="@dimen/apps_details_parent_padding"
    android:paddingEnd="@dimen/apps_details_parent_padding"
    tools:context=".appointmentNew.AppointmentDetailsActivity">

    <include
        android:id="@+id/toolbar_common"
        layout="@layout/toolbar_common_new2_ui2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/apps_details_toolbar_margin_start"
        android:layout_marginEnd="@dimen/apps_details_toolbar_margin_end" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardview_head"
        android:layout_width="match_parent"
        android:layout_height="@dimen/apps_details_profile_card_height"
        android:layout_below="@+id/toolbar_common"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="@dimen/apps_details_profile_card_margin_start_end"
        android:layout_marginTop="@dimen/apps_details_profile_card_margin_top"
        android:layout_marginEnd="@dimen/apps_details_profile_card_margin_start_end"
        android:elevation="@dimen/apps_details_profile_card_elevation"
        app:cardCornerRadius="@dimen/apps_details_profile_card_corner_radius"
        app:cardElevation="@dimen/apps_details_profile_card_corner_elevation">

        <LinearLayout
            android:id="@+id/frame_10034"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/apps_details_profile_margin"
            android:orientation="horizontal"
            android:weightSum="1">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <androidx.cardview.widget.CardView
                    android:id="@+id/ivAppointmentProfile"
                    android:layout_width="@dimen/apps_details_profile_iv_height"
                    android:layout_height="@dimen/apps_details_profile_iv_width"
                    android:layout_alignParentTop="true"
                    android:elevation="0dp"
                    app:cardCornerRadius="@dimen/apps_details_profile_iv_radius">

                    <ImageView
                        android:id="@+id/profile_image_app_details"
                        android:layout_width="@dimen/apps_details_profile_iv_height"
                        android:layout_height="@dimen/apps_details_profile_iv_width"
                        android:src="@drawable/avatar1" />
                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/card_profile_details_margin_start"
                    android:layout_toStartOf="@+id/call_chat_action_ll"
                    android:layout_toEndOf="@+id/ivAppointmentProfile"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/patname_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center_horizontal|center_vertical"
                            android:lineSpacingExtra="-3sp"
                            android:maxLength="15"
                            android:maxLines="1"
                            android:text="@string/fu_visit_name"
                            android:textAppearance="@style/label_16sp"
                            android:textColor="@null"
                            android:textSize="@dimen/card_profile_name_text_size" />

                        <TextView
                            android:id="@+id/gender_age_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/cp_gender_margin_start"
                            android:layout_toEndOf="@+id/patname_txt"
                            android:lineSpacingExtra="-2sp"
                            android:text="@string/misc_txt"
                            android:textAppearance="@style/label_misc_txt_14sp"
                            android:textColor="@null"
                            android:textSize="@dimen/card_profile_name_text_size" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="@dimen/fu_male_margin_start"
                            android:onClick="editPatientInfo"
                            android:padding="@dimen/margin_4"
                            android:src="@drawable/ic_icno8_edit" />

                    </LinearLayout>

                    <!-- Caption -->
                    <TextView
                        android:id="@+id/openmrsID_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_marginTop="@dimen/cp_openmrsid_margin_top"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/fu_openmrsID"
                        android:textAppearance="@style/caption_fu_openmrsId"
                        android:textColor="@null"
                        android:textSize="@dimen/cp_openmrsid_text_size" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/call_chat_action_ll"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_call_patient_app"
                        android:layout_width="36dip"
                        android:layout_height="36dip"
                        android:background="@drawable/ui2_ic_bg_contact_icons"
                        android:padding="@dimen/padding_10"
                        android:src="@drawable/ui2_ic_call_green" />

                    <ImageView
                        android:id="@+id/iv_whatsapp_patient_app"
                        android:layout_width="36dip"
                        android:layout_height="36dip"
                        android:layout_marginStart="@dimen/cp_option_margin_start"
                        android:background="@drawable/ui2_ic_bg_contact_icons"
                        android:padding="@dimen/padding_10"
                        android:src="@drawable/ui2_ic_whatsapp_green" />

                </LinearLayout>

            </RelativeLayout>

            <!--2nd-->

        </LinearLayout>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cardview_head"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="@dimen/cp_vs_margin_start"
        android:layout_marginTop="@dimen/cp_vs_margin_top"
        android:layout_marginEnd="@dimen/cp_vs_margin_start"
        android:elevation="16dp"
        app:cardCornerRadius="@dimen/margin_5"
        app:cardElevation="@dimen/margin_5">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/cp_card_margin_updown"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/llPriorityTagAppointmentDetails"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_gravity="top"
                    android:layout_marginEnd="@dimen/cp_margin_start"
                    android:background="@drawable/ui2_frame_priority"
                    android:contentDescription="Appointment Details Screen Priority Tag Linear Layout"
                    android:visibility="gone"
                    tools:visibility="visible">

                    <TextView
                        android:id="@+id/fu_priority_tag"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:lineSpacingExtra="0sp"
                        android:text="@string/priority"
                        android:textAlignment="center"
                        android:textAllCaps="true"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        android:textStyle="bold" />

                </LinearLayout>

                <TextView
                    android:id="@+id/chief_complaint_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="3sp"
                    android:text="Headache and body pain"
                    android:textAppearance="@style/details_head1"
                    android:textColor="@null"
                    android:textSize="@dimen/cp_name_txt_size" />

                <TextView
                    android:id="@+id/visitID_appointment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/chief_complaint_txt"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="3sp"
                    android:text="Visit ID : 987654jK"
                    android:textAppearance="@style/details_head2"
                    android:textColor="@null"
                    android:textSize="@dimen/cp_male_textsize" />

                <ImageButton
                    android:id="@+id/fu_progress_indicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/visitID_appointment"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_marginTop="@dimen/cp_card_margin_updown"
                    android:background="@null"
                    android:src="@drawable/details_progressindic" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/state_prev_scheduled_on"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fu_progress_indicator"
                android:layout_marginTop="@dimen/cp_card_margin_updown"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_appointment_time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/ps_app_time_margin_start"
                    android:layout_toEndOf="@+id/fu_details_calendar"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                    android:text="@string/previously_scheduled_on"
                    android:textAppearance="@style/fu_detiails_date"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/cp_male_textsize" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_appointment_time1"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_marginTop="@dimen/tv_reschedule_time_margin_top"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_prev_app_date"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/tv_reschedule1_time_margin_start"
                        android:layout_toEndOf="@+id/fu_details_calendar"
                        android:drawableStart="@drawable/ui2_ic_icons8_calendar_1"
                        android:drawablePadding="@dimen/tv_reschedule1_time_drawable_padding"
                        android:drawableTint="@color/textColorBlack"
                        android:gravity="center_vertical"
                        android:lineHeight="@dimen/tv_reschedule1_time_line_ht"
                        android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                        android:text="21 July 2022"
                        android:textAppearance="@style/fu_detiails_date"
                        android:textColor="@color/textColorBlack"
                        android:textSize="@dimen/cp_male_textsize" />

                    <TextView
                        android:id="@+id/tv_prev_app_time"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="60dp"
                        android:layout_toEndOf="@+id/fu_details_calendar"
                        android:drawableStart="@drawable/ui2_ic_icons8_time_1"
                        android:drawablePadding="@dimen/tv_reschedule1_time_drawable_padding"
                        android:drawableTint="@color/textColorBlack"
                        android:gravity="center_vertical"
                        android:lineHeight="@dimen/tv_reschedule1_time_line_ht"
                        android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                        android:text="10:00 am"
                        android:textAppearance="@style/fu_detiails_date"
                        android:textColor="@color/textColorBlack"
                        android:textSize="@dimen/fu_male_textsize" />
                </LinearLayout>


            </RelativeLayout>

            <!-- ui for state appointment started-->
            <RelativeLayout
                android:id="@+id/state_appointment_started"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fu_progress_indicator"
                android:layout_marginTop="@dimen/rt_card_margin_updown"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_reschedule_on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/tv_reschedule_time_margin_start"
                    android:layout_toEndOf="@+id/fu_details_calendar"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                    android:text="@string/rescheduled_on"
                    android:textAppearance="@style/fu_detiails_date"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/cp_male_textsize"
                    android:visibility="gone" />


                <LinearLayout
                    android:id="@+id/ll_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_reschedule_on"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_marginTop="@dimen/tv_reschedule_time_margin_top"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/visit_startDate_appointment"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/tv_reschedule1_time_margin_start"
                        android:layout_toEndOf="@+id/fu_details_calendar"
                        android:drawableStart="@drawable/ui2_ic_icons8_calendar_1"
                        android:drawablePadding="@dimen/tv_reschedule1_time_drawable_padding"
                        android:drawableTint="@color/textColorBlack"
                        android:gravity="center_vertical"
                        android:hint="21 July 2022"
                        android:lineHeight="@dimen/tv_reschedule1_time_line_ht"
                        android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                        android:textAppearance="@style/fu_detiails_date"
                        android:textColor="@color/textColorBlack"
                        android:textSize="@dimen/cp_male_textsize" />

                    <TextView
                        android:id="@+id/tv_starttime_appointment"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="60dp"
                        android:layout_toEndOf="@+id/fu_details_calendar"
                        android:drawableStart="@drawable/ui2_ic_icons8_time_1"
                        android:drawablePadding="@dimen/tv_reschedule1_time_drawable_padding"
                        android:drawableTint="@color/textColorBlack"
                        android:gravity="center_vertical"
                        android:hint="10:00 am"
                        android:lineHeight="@dimen/tv_reschedule1_time_line_ht"
                        android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                        android:textAppearance="@style/fu_detiails_date"
                        android:textColor="@color/textColorBlack"
                        android:textSize="@dimen/cp_male_textsize" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_appointment_time_details"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ll_1"
                    android:layout_marginStart="@dimen/tv_reschedule_time_margin_start"
                    android:layout_marginTop="@dimen/tv_reschedule_time_margin_top"
                    android:drawableStart="@drawable/ui2_ic_time"
                    android:drawablePadding="@dimen/tv_reschedule1_time_drawable_padding"
                    android:drawableTint="@color/colorPrimary1"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                    android:text="Started 1 minute ago"
                    android:textAppearance="@style/fu_detiails_date"
                    android:textColor="@color/colorPrimary1"
                    android:textSize="@dimen/cp_male_textsize" />
            </RelativeLayout>


            <View
                android:id="@+id/divider"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/state_appointment_started"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginTop="@dimen/cp_card_margin_updown"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:layout_marginBottom="@dimen/cp_card_margin_updown"
                android:background="@color/notifi_divider_color" />


            <RelativeLayout
                android:id="@+id/layout_patient_history"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/divider_2"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:layout_marginBottom="@dimen/cp_card_margin_updown"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/ivAppointmentPatientHistory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:src="@drawable/ui2_ic_patient_history" />

                <!-- label-text -->
                <TextView
                    android:id="@+id/visitDetails_textq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_toEndOf="@+id/ivAppointmentPatientHistory"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                    android:text="@string/view_patient_history"
                    android:textAppearance="@style/visitDetails_text"
                    android:textColor="@null"
                    android:textSize="@dimen/cp_name_txt_size" />

                <ImageButton
                    android:id="@+id/icon_arrowRight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:src="@drawable/arrow_right_icon8" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/visitDetails_textq"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_marginTop="@dimen/dividerqq_margin_top"
                    android:layout_marginEnd="@dimen/cp_margin_start"
                    android:background="@color/notifi_divider_color" />
            </RelativeLayout>


            <LinearLayout
                android:id="@+id/ll_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/divider"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:orientation="horizontal"
                android:weightSum="2">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2">

                    <ImageView
                        android:id="@+id/profile"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/icons_appoint_height"
                        android:layout_centerVertical="true"
                        android:background="@null"
                        android:src="@drawable/ic_grp_general_phys" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/cp_margin_start"
                        android:layout_toEndOf="@+id/profile"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/dr_speciality_appointment"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/ad_male_margin_start"
                            android:gravity="center_vertical"
                            android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                            android:text="@string/speciality"
                            android:textAppearance="@style/details_head2"
                            android:textColor="@color/textColorBlack"
                            android:textSize="@dimen/ad_title_text_size" />
                        <!-- Caption -->
                        <TextView
                            android:id="@+id/dr_speciality"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/ad_male_margin_start"
                            android:gravity="center_vertical"
                            android:lineSpacingExtra="0sp"
                            android:text="@string/dr_speciality"
                            android:textAppearance="@style/details_head2"
                            android:textColor="@null"
                            android:textSize="@dimen/cp_male_textsize" />

                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/layout_contact_action"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/ad_icons_margin_top"
                    android:orientation="horizontal"
                    tools:ignore="ExtraText">

                    <ImageView
                        android:id="@+id/iv_call_doctor_app"
                        android:layout_width="@dimen/cp_option_icon_width"
                        android:layout_height="@dimen/cp_option_icon_height"
                        android:layout_marginStart="@dimen/cp_option_margin_start"
                        android:background="@drawable/ui2_ic_bg_contact_icons"
                        android:padding="@dimen/cp_option_icon_padding_whts"
                        android:src="@drawable/ui2_ic_call_green" />

                    <ImageView
                        android:id="@+id/iv_whatsapp_doctor_app"
                        android:layout_width="@dimen/cp_option_icon_width"
                        android:layout_height="@dimen/cp_option_icon_height"
                        android:layout_marginStart="@dimen/cp_option_margin_start"
                        android:background="@drawable/ui2_ic_bg_contact_icons"
                        android:padding="@dimen/cp_option_icon_padding_whts"
                        android:src="@drawable/ui2_ic_whatsapp_green" />

                </LinearLayout>

            </LinearLayout>

            <View
                android:id="@+id/divider_2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/ll_2"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginTop="@dimen/cp_card_margin_updown"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:layout_marginBottom="@dimen/cp_card_margin_updown"
                android:background="@color/notifi_divider_color" />

            <!-- Frame 2608662 -->
            <RelativeLayout
                android:id="@+id/layout_visit_summary"
                android:layout_width="match_parent"
                android:layout_height="@dimen/icons_appoint_height"
                android:layout_below="@+id/divider_2"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginEnd="@dimen/cp_margin_start">

                <ImageView
                    android:id="@+id/icon_visit_details"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/icons_appoint_height"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:src="@drawable/ic_visit_details_icon8" />

                <!-- label-text -->
                <TextView
                    android:id="@+id/visitDetails_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/cp_margin_start"
                    android:layout_toEndOf="@+id/icon_visit_details"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/reschedule_time_line_spacing"
                    android:text="@string/visitDetails_text"
                    android:textAppearance="@style/visitDetails_text"
                    android:textColor="@null"
                    android:textSize="@dimen/ad_title_text_size" />

                <ImageView
                    android:id="@+id/iv_drawer_visit_summary"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:src="@drawable/arrow_right_icon8" />
            </RelativeLayout>

            <View
                android:id="@+id/divider_3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/ll_3"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginTop="@dimen/cp_card_margin_updown"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:layout_marginBottom="@dimen/cp_card_margin_updown"
                android:background="@color/notifi_divider_color"
                android:visibility="visible" />

            <LinearLayout
                android:id="@+id/layout_visit_summary_buttons"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/divider_3"
                android:layout_marginStart="@dimen/cp_margin_start"
                android:layout_marginEnd="@dimen/cp_margin_start"
                android:layout_marginBottom="@dimen/cp_card_margin_updown"
                android:orientation="horizontal"
                android:visibility="gone"
                android:weightSum="1">

                <Button
                    android:id="@+id/btn_schedule_appointment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/btn_visit_margin_bottom"
                    android:background="@drawable/common_button_bg_primary_new_ui2"
                    android:letterSpacing="0.02"
                    android:text="@string/schedule_appointment"
                    android:textColor="@color/white"
                    android:textSize="@dimen/btn_visit_text_size"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/btn_cancel_appointment"
                    android:layout_width="0dp"
